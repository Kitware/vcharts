{
  "devDependencies": {
    "exorcist": "^0.4.0",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3",
    "uglify-js": "^2.4.24"
  },
  "name": "vcharts",
  "description": "Reusable [Vega](http://vega.github.io/vega/) charts.",
  "version": "0.1.6",
  "main": "src/index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "d3": "^3.5.6",
    "vega": "^2.2.6"
  },
  "scripts": {
    "lint": "jshint src/",
    "build": "browserify src/index.js -d -s vcharts | exorcist vcharts.js.map > vcharts.js",
    "postbuild": "uglifyjs vcharts.js -cm > vcharts.min.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XDATA-Year-3/vcharts.git"
  },
  "keywords": [
    "vega",
    "chart",
    "graph",
    "visualization"
  ],
  "author": "Jeffrey Baumes <jeff.baumes@kitware.com> (http://www.kitware.com/company/team/baumes.html)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/XDATA-Year-3/vcharts/issues"
  },
  "homepage": "https://github.com/XDATA-Year-3/vcharts#readme"
}
